{
  "name": "recreactive",
  "version": "1.0.0",
  "description": "React - recreational sessions",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/unboxit/recreactive.git"
  },
  "bugs": {
    "url": "http://github.com/unboxit/recreactive.git/issues"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "scripts": {
    "build": "webpack",
    "bithound": "bithound check git@github.com:unboxit/recreactive.git",
    "eslint": "eslint --fix ./src --ext=.js --ext=.jsx",
    "jscs": "jscs ./src --esnext",
    "lint": "npm run jscs && npm run eslint",
    "prestart": "npm run build",
    "start:dev": "hjs-dev-server",
    "test": "mocha client/*.spec.js --opts .mocha.opts --reporter min",
    "test:watch": "mocha client/*.spec.js --opts .mocha.opts --watch"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.5.2",
    "bithound": "^1.0.0",
    "eslint": "^2.2.0",
    "expect": "^1.14.0",
    "expect-jsx": "^2.3.0",
    "file-loader": "^0.8.5",
    "hjs-webpack": "^7.2.1",
    "html-loader": "^0.4.3",
    "jscs": "^2.9.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7"
  }
}
